<p style="color: green"><%= notice %></p>

<h1>Articles</h1>
<%= link_to "New Articles", new_article_path %>

<div id="articles">
  <% @articles.each do |article| %>
    <%= render article %>
    <p>
      <%= link_to "Show", article %> |
      <%= link_to "Edit", edit_article_path(article) %> |
      <%= link_to "Delete", article_path(article), method: :delete %>
    </p>
  <% end %>
</div>

